const std::string& biblio::Journal::reqNom() const
{
	return m_nom;
}

int biblio::Journal::reqNumero() const
{
	return m_numero;
}

int biblio::Journal::reqPage() const
{
	return m_page;
}

int biblio::Journal::reqVolume() const
{
	return m_volume;
}
