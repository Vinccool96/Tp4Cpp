const std::string& biblio::Ouvrage::reqEditeur() const
{
	return m_editeur;
}

void biblio::Ouvrage::asgEditeur(const std::string& editeur)
{
	m_editeur = editeur;
}

const std::string& biblio::Ouvrage::reqVille() const
{
	return m_ville;
}

void biblio::Ouvrage::asgVille(const std::string& ville)
{
	m_ville = ville;
}
