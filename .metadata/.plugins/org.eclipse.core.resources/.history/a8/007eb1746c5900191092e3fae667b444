#include "Ouvrage.h"
#include "validationFormat.h"
#include <sstream>

namespace biblio
{

	const std::string& Ouvrage::reqEditeur() const
	{
		return m_editeur;
	}

	void Ouvrage::asgEditeur(const std::string& editeur)
	{
		m_editeur = editeur;
	}

	const std::string& Ouvrage::reqVille() const
	{
		return m_ville;
	}

	void Ouvrage::asgVille(const std::string& ville)
	{
		m_ville = ville;
	}
} // namespace biblio
